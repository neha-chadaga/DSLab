#include<stdio.h>
int main()
{
  int id,age,marks;
  printf("Enter Student ID:\n");
  scanf("%d",&id);
  printf("Enter your age:\n");
  scanf("%d",&age);
  printf("Enter your marks in the qualifying exam:\n");
  scanf("%d",&marks);
  int i=0;
  if((age>20)&&(marks>=0 && marks<=100))
  {
    i=1;
    printf("Verification successful\n");
  }
  else
  {
    printf("Student data is invalid\n");
    exit(0);
  }
if((i==1)&&(marks>=65))
{
  printf("You are qualified for admission\n");
}
else
printf("Sorry,you are not eligible for admission\n");
}
